{
  "name": "jdbench-portfolio",
  "version": "1.0.0",
  "description": "Notable work completed by Justin Bench",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --collectCoverage",
    "lint:pre-commit" : "eslint --ignore-path ./.eslintignore $(git diff --staged --name-only | grep -E '.ts$|.tsx$' | xargs)",
    "format:pre-commit": "prettier --check $(git diff --staged --name-only | grep -E '.ts$|.tsx$|.scss$' | xargs)"
  },
  "husky": {
      "hooks": {
          "pre-commit": "npm run format:pre-commit && npm run lint:pre-commit"
      }
    },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdbench/jdbench-portfolio.git"
  },
  "author": "jdbench",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jdbench/jdbench-portfolio/issues"
  },
  "dependencies": {
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "~8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "homepage": "https://github.com/jdbench/jdbench-portfolio#readme"
}
